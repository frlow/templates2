    "solid-start-node": "^0.2.19",
    "typescript": "^4.9.4",
    "vite": "^3.2.5",
    "yaml": "^2.2.1",
    "tsx": "^3.12.3"
  },
  "dependencies": {
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.7.0",
    "solid-js": "^1.6.11",
    "solid-start": "^0.2.21",
    "solid-styled-components": "^0.28.5",
    "undici": "^5.15.1",
